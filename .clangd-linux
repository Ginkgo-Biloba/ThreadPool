CompileFlags:
  Compiler: /usr/bin/x86_64-linux-gnu-g++
  Add:
    - -D_WIN32_WINNT=_WIN32_WINNT_VISTA
    - -std=gnu++14
    - -masm=intel
    - -march=native
    - -O0
    - -ggdb
    - -Wall
    - -Wextra
    - -pedantic
    - -Werror
    - -Wcast-align
    - -Wconversion
    - -Wno-sign-conversion
    - -Wpointer-arith
    - -Wshadow
    - -Wreturn-type

Index:
  Background: Skip

Diagnostics:
  Suppress: builtin_definition
  UnusedIncludes: None


InlayHints:
  BlockEnd: No
  Designators: Yes
  Enabled: Yes
  ParameterNames: Yes
  DeducedTypes: Yes
  TypeNameLimit: 24

Hover:
  ShowAKA: Yes
